<Window x:Class="Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Name="speedyCanvas" HorizontalAlignment="Left" Height="401" Margin="10,10,0,0" VerticalAlignment="Top" Width="773">
            <TextBox x:Name="speedLimitTextBox" Height="23" Canvas.Left="164" TextWrapping="Wrap" Text="" Canvas.Top="141" Width="120"/>
            <TextBox x:Name="yourSpeedTextBox" Height="23" Canvas.Left="478" TextWrapping="Wrap" Text="" Canvas.Top="141" Width="120"/>
            <Label x:Name="speedLimitLabel" Content="Speed Limit" Canvas.Left="87" Canvas.Top="137"/>
            <Label x:Name="yourSpeedLabel" Content="Your Speed" Canvas.Left="403" Canvas.Top="141"/>
            <Button x:Name="goButton" Click="goButton_Click" Content=" VROOM VROOM " Canvas.Left="302" Canvas.Top="227" Width="119"/>
            <Label x:Name="statusLabel" Content="Let's Go For a Drive" Canvas.Left="308" Canvas.Top="73"/>
        </Canvas>

    </Grid>
</Window>
